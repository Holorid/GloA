a = int(input())

if a >= 2:
    maximum = int(input())
    minimum = int(input())
    for i in range(0, a - 2):
        num = int(input())
        if num > maximum:
            maximum = num
        if num < minimum:
            minimum = num
    print('maximum', maximum)
    print('minimum', minimum)
else:
    print('Ошибка ввода')

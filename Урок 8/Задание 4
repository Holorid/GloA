num = int(input())
maximum = num % 10
minimum = num % 10

while num != 0:
    l_num = num % 10
    print(l_num)

    if maximum < l_num:
        maximum = l_num
        
    if minimum > l_num:
        minimum = l_num
    num = num // 10
print('Максимальная цифра равна: ', maximum, '\n', 'Минимальная цифра равна: ', minimum, sep='')
